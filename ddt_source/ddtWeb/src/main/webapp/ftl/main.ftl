.leftul li{list-style:none;width:196px;height: 50px;text-align: center;border-bottom: 2px solid #6c3;}
.leftul a{display: block;text-decoration: none;height: 50px;line-height: 50px;width: 196px;color: #999;}
.leftul li:hover{background: #6c3;}
.leftul a:hover{color: #fff;}
